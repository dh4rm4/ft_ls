#include "../include/ft_ls.h"

void	ls_free(t_elem *all)
{
	if (FILE_NAME)
		free(FILE_NAME);
	if (PATH)
		free(PATH);
	if (OLD_PATH)
		free(OLD_PATH);
	if (HARD_LINK)
		free(HARD_LINK);
	
}